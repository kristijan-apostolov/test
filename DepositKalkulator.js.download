                        + this._getPeriodName(this._currCalcInt.Terms[rate].Period) + "</option>");
                }
                else {
                    $("#Period").append("<option value='" + this._currCalcInt.Terms[rate].Period + "' data-interest='" + this._currCalcInt.Terms[rate].Interest + "'>"
                        + this._getPeriodName(this._currCalcInt.Terms[rate].Period) + "</option>");
                }

            }
        }
